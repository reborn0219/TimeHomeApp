 # Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TimeHomeApp' do
    # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
    # use_frameworks!
    
    # Pods for TimeHomeApp
    
    #3D滚动视图控件
    pod 'iCarousel', '~> 1.8.3'
    #ActionSheet控件
    pod 'FSActionSheet', '~> 1.0.1'
    
    #丰富的分类工具库
    pod 'YYCategories', '~> 1.0.4'
    
    #提供数据模型转换
    pod 'YYModel', '~> 1.0.4'
    pod 'MJExtension', '~> 3.0.13'
    
    #键盘管理工具
    pod 'IQKeyboardManager', '~> 6.0.3'
    
    #上拉加载，下拉刷新
    pod 'MJRefresh', '~> 3.1.15.3'
    
    #网络图片加载工具
    pod 'SDWebImage', '~> 4.3.3'
    
    #GIF加载工具
    pod 'YLGIFImage', '~> 0.11'
    #进度条
    pod 'SVProgressHUD', '~> 2.2.5'
    
    #AutoLayout工具
    pod 'Masonry', '~> 1.1.0'
    
    #Alert工具
    pod 'SCLAlertView-Objective-C', '~> 1.1.3'
    
    #友盟统计
    pod 'UMCCommonLog'
    pod 'UMCCommon'
    pod 'UMCAnalytics'
    
    #百度地图
    pod 'BaiduMapKit'
    pod 'BMKLocationKit'
    
    #云数据库
    pod 'BmobSDK'
    
    target 'TimeHomeAppTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end

